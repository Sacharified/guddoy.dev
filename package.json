{
  "name": "guddoy.dev",
  "version": "1.0.0",
  "description": "Sacha Guddoy's Website",
  "main": "index.js",
  "scripts": {
    "test": "jest ./src",
    "test:ci": "npm run test -- --ci --collect-coverage",
    "test:watch": "jest ./src --watch",
    "test-ua": "npm run netlify-dev > /dev/null | CYPRESS_baseUrl=http://localhost:8888 cypress open",
    "test-ua:ci": "netlify deploy --json > deploy.json && CYPRESS_baseUrl=$(jq .deploy_url deploy.json | tr -d '\"') cypress run",
    "dev": "next ./src",
    "netlify-dev": "netlify dev",
    "build": "next build ./src",
    "start": "npm run export && http-server ./out -p 3000 ",
    "export": "npm run build && next export ./src --outdir='./out'",
    "deploy": "netlify deploy",
    "lint": "eslint ./src ./cypress --fix",
    "postinstall": "npm-recursive-install --rootDir=./functions"
  },
  "author": "Sacha Guddoy",
  "license": "ISC",
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.1.0",
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "@contentful/rich-text-types": "^13.4.0",
    "@material-ui/core": "^4.0.0-rc.0",
    "anagram-solver": "^1.0.1",
    "contentful": "^7.8.2",
    "dateformat": "^3.0.3",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.13",
    "material-design-icons": "^3.0.1",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.4",
    "mobx-state-tree": "^3.14.1",
    "next": "^9.0.3",
    "next-images": "^1.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-syntax-highlighter": "^11.0.2"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/preset-env": "^7.5.5",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "cypress": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "esm": "^3.2.25",
    "http-server": "^0.11.2",
    "jest": "^24.8.0",
    "jest-junit": "^7.0.0",
    "memfs": "^2.15.5",
    "netlify": "^2.4.8",
    "recursive-install": "^1.4.0"
  }
}
